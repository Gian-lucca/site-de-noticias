
{% include directory ~ "/partials/header.html.twig" %}

{% if page.sliderPrincipal  %}
    <div class="slider-fixo" data-aos="zoom-in" data-aos-duration="2000">
        {{ page.sliderPrincipal }}
		
    </div><!-- /slider-fixo -->
    
{% endif %}

{% if page.bannerPrincipal %}
    <section class="banner branco">
        <div class="slider-fixo" data-aos="zoom-in" data-aos-duration="2000">
            <div class="banner-principal">
                {{ page.bannerPrincipal }}
            </div>
        </div>
    </section>
{% endif %}

{% if page.sliderEstrutura %}
    <section data-aos="fade-left" class="programas-acoes branco aos-init aos-animate">
        <div class="section-container estrutura">    
            <div class="titulo-secao">
                <span>Estrutura</span>
            </div>       
            {{ page.sliderEstrutura }}
        </div>    
    </section><!-- /estrutura -->
{% endif %}

{% if page.noticiasPrincipal %}
    <section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
		<div class="titulo-secao">
			<span>Notícias</span>
		</div> 
        {{ page.noticiasPrincipal }}
        <div class="lista-noticias-line">
                <button class="lista-noticias">
                <a id="A3" href="./noticias">Veja a lista completa de notícias</a>
                </button>
            </div>
        </div>
    </section>
    
{% endif %}


<section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
	<div class="titulo-secao" class="painelContainer-wrap">
		<span>PAINEL ECONÔMICO</span>
	</div> 
</section>

<div class="painelContainer-wrap">
		{% if page.pibbrasil %}
			<section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
				<div class="titulo-pe" class="painel-titulo">
					<span>PIB Brasil</span>
				</div> 
				{{ page.pibbrasil }}
			</section>
			
		{% endif %}

		{% if page.moedas %}
			<section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
				<div class="titulo-pe" class="painel-titulo">
					<span>Moedas</span>
					</div> 
					{{ page.moedas }}
				
			</section>
			
		{% endif %}

		{% if page.taxas %}
			<section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
				<div class="titulo-pe" class="painel-titulo">
					<span>Taxas</span>
					</div> 
					{{ page.taxas }}
				
			</section>
			
		{% endif %}

		{% if page.indice %}
			<section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
				<div class="titulo-pe" class="painel-titulo">
					<span>Índice</span>
					</div> 
					{{ page.indice }}
				
			</section>
			
		{% endif %}

		{% if page.balancacomerical %}
			<section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
				<div class="titulo-pe" class="painel-titulo">
					<span>Balança Comercial</span>
					</div> 
					{{ page.balancacomerical }}
				
			</section>
			
		{% endif %}

		{% if page.salariominimo %}
			<section class="banner branco noticias" class="programas-acoes branco aos-init aos-animate">
				<div class="titulo-pe" class="painel-titulo">
					<span>Salário Mínimo</span>
					</div> 
					{{ page.salariominimo }}
				
			</section>
			
		{% endif %}
		
</div>	




{% if page.videosPrincipal %}
    <section id="video" data-aos="fade-right" class="programas-acoes branco aos-init aos-animate"  data-aos-easing="ease-out-cubic" data-aos-duration="2000">
        <div class="section-container" id="video-section">
            <div class="titulo-secao"><span>Vídeos</span></div>
                {{ page.videosPrincipal }}
            <div class="lista-noticias-line">
                <button class="lista-noticias">
                <a id="A3" href="./video_principal_youtube">Veja a lista completa de vídeos</a>
                </button>
            </div>
        </div>
    </section>
{% endif %}

{% if page.publicacoes %}
    <section data-aos="fade-left" class="programas-acoes branco aos-init aos-animate">
        <div class="section-container">
            <div class="titulo-secao">
                <span>Publicações</span>
            </div>
            {{ page.publicacoes }}
			<div class="lista-noticias-line">
                <button class="lista-noticias">
                <a id="A3" href="/publicacoes">Veja a lista completa de Publicações</a>
                </button>
            </div>
        </div>
    </section>
    <hr class="gradiente">
{% endif %}



{% if page.flipCard %}
    <section class="section-container aos-init aos-animate" data-aos="flip-down" data-aos-easing="ease-out-cubic">
    	{{ page.flipCard  }}
    </section>
    <hr class="gradiente">
{% endif %}

{% if page.dadosPrincipal %}
    <section class="programas-acoes branco" data-aos="fade-up-right" data-aos-easing="ease-out-cubic" data-aos-duration="2000" class="aos-init aos-animate">
        <div class="section-container">
            <div class="titulo-secao">
                <span>ISP Dados</span>
                <p></p>
            </div>
            <div class="dados-principal">
                {{ page.dadosPrincipal }}
            </div>

        </div>
    </section>
<hr class="gradiente">
{% endif %}

