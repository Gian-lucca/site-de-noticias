{% if page.sliderSemTempo  %}
    <div class="slider-fixo" data-aos="zoom-in" data-aos-duration="2000">
        {{ page.sliderSemTempo }}       
    </div><!-- /slider-fixo -->
    
{% endif %}

{% if page.sliderUteis %}
    <section data-aos="fade-left" class="programas-acoes branco aos-init aos-animate">
        <div class="section-container">
            <div class="titulo-secao">
                <span>Links Úteis</span>
            </div>
            {{ page.sliderUteis }}
			<div class="lista-links">
                <button class="lista-noticias">
                <a id="A3" href="./publicacoes">Veja a lista completa de Links Uteis</a>
                </button>
            </div>
        </div>
    </section><!-- /linkUteis -->
    <hr class="gradiente">
{% endif %}


{% if page.sliderSecundario  %}
    <div class="slider-fixo" data-aos="zoom-in" data-aos-duration="2000">
        {{ page.sliderSecundario }}
		<div class="lista-noticias-line">
              
            </div>
        </div>
    </div><!-- /slider-fixo -->
    
{% endif %}

{% if page.sliderServicos %}
    <section data-aos="fade-left" class="programas-acoes branco aos-init aos-animate">
        <div class="section-container">    
            <div class="titulo-secao">
                <span>Serviços</span>
            </div>       
            {{ page.sliderServicos }}
        </div>    
    </section><!-- /serviços -->
{% endif %}

